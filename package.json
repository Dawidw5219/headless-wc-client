{
  "name": "headless-wc-client",
  "version": "1.0.4",
  "description": "WooCommerce Headless Solution",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "git": "git add . && git commit -m 'update' && git push origin main",
    "deploy": "npm run build && npm publish && npm run git"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dawidw5219/headless-wc-client.git"
  },
  "keywords": [
    "woocommerce",
    "wordpress",
    "ecommerce",
    "headless",
    "rest",
    "node",
    "typescript",
    "api"
  ],
  "author": "Dawid Wiewi√≥rski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dawidw5219/headless-wc-client/issues"
  },
  "homepage": "https://github.com/Dawidw5219/headless-wc-client#readme",
  "devDependencies": {
    "@types/node": "^22.5.5",
    "tsup": "^8.0.2",
    "typedoc": "^0.25.9",
    "typescript": "^5.3.3"
  }
}
